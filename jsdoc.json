{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs/)"
    },
    "plugins": [
        "./node_modules/jsdoc/plugins/markdown"
    ],
    "opts": {
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "docdash": {
        "static": true,
        "sort": true
    }
}